{
	"name": "@birdhouse/website",
	"version": "0.0.1",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:webpack": "webpack serve",
		"dev:routify": "routify",
		"clean": "rimraf ./build",
		"build": "npm run clean && npm run gen && routify -b && cross-env NODE_ENV=production webpack",
		"preview": "sirv build --single",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"gen": "graphql-codegen --config codegen.yml",
		"postinstall": "patch-package"
	},
	"spank": {
		"entrypoint": "build/index.html",
		"script": "build/build/bundle.js",
		"inlineDynamicImports": true
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.1.1",
		"@graphql-codegen/introspection": "2.1.0",
		"@graphql-codegen/typescript": "2.1.2",
		"@graphql-codegen/typescript-generic-sdk": "^2.1.2",
		"@graphql-codegen/typescript-graphql-request": "^4.1.2",
		"@graphql-codegen/typescript-operations": "2.1.2",
		"@roxi/routify": "^2.18.3",
		"@tsconfig/svelte": "^1.0.13",
		"@types/lodash": "^4.14.177",
		"@types/node": "^16.11.12",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"copy-webpack-plugin": "^9.0.1",
		"cross-env": "^7.0.3",
		"cross-fetch": "^3.1.4",
		"css-loader": "^6.2.0",
		"dotenv-webpack": "^7.0.3",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"extract-files": "^11.0.0",
		"form-data": "^4.0.0",
		"graphql": "^15.5.2",
		"graphql-request": "^3.5.0",
		"graphql-tag": "^2.12.5",
		"mini-css-extract-plugin": "^1.3.4",
		"node-html-parser": "^5.2.0",
		"npm-run-all": "^4.1.5",
		"npm-watch": "^0.11.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rimraf": "^3.0.2",
		"sass": "^1.38.2",
		"sass-loader": "^12.1.0",
		"sirv-cli": "^1.0.14",
		"spank": "^1.9.0",
		"svelte": "^3.43.1",
		"svelte-check": "^2.0.0",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.8.0",
		"ts-loader": "^9.2.5",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"webpack": "^5.16.0",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.1.0"
	},
	"dependencies": {
		"@date-io/date-fns": "^2.11.0",
		"date-fns": "^2.26.0",
		"flatpickr": "^4.6.9",
		"graphql-config": "^4.1.0",
		"lodash": "^4.17.21",
		"objects2csv": "^1.1.0",
		"patch-package": "^6.4.7",
		"svelte-flatpickr": "^3.2.4",
		"svelte-inclusive-datepicker": "^1.0.2"
	}
}
